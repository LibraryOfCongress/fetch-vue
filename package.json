{
  "name": "quasar-pwa",
  "version": "0.0.1",
  "description": "Inventory Management Software Development PWA bult using the Quasar Framework",
  "productName": "Fetch App",
  "author": "zegle <zegle@loc.gov>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "lint:fix": "eslint --ext .js,.vue ./ --fix",
    "lint-staged-commit": "eslint --quiet --ext .js,.vue --fix $(git diff --name-only --diff-filter=d HEAD | xargs)",
    "dev": "quasar dev",
    "build": "quasar build",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.7.4",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.3.0",
    "@quasar/quasar-app-extension-testing": "^2.1.1",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.3.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.44",
    "@vue/test-utils": "^2.0.0",
    "vitest": "^0.31.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
